{
    "Lua.workspace.preloadFileSize": 1000,
    "suggest.noselect": true,
    "languageserver": {
        "go": {
            "command": "gopls",
            "rootPatterns": ["go.mod"],
            "trace.server": "verbose",
            "filetypes": ["go"]
        },
        "dockerfile": {
            "command": "docker-langserver",
            "filetypes": ["dockerfile"],
            "args": ["--stdio"]
        }
    },
    "Lua.telemetry.enable": true
}
